<Window x:Class="Customer_integ.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Customer_integ"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Print All" HorizontalAlignment="Left" Margin="380,235,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="125,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="115" Width="220" Background="#FFD0FFF0"/>
        <Button x:Name="next" Click ="next_Click_1"  Content="Next" HorizontalAlignment="Left" Margin="270,185,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="prev" Content="Prev" Click="prev_Click"  HorizontalAlignment="Left" Margin="125,185,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button1" Content="Do Not Print" Click="button1_Click_1" HorizontalAlignment="Left" Margin="195,220,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button2" Content="Revert Back" Click="button2_Click" HorizontalAlignment="Left" Margin="195,255,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
